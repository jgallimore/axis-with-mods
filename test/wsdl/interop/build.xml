               deployscope="none"
               serverSide="yes"
               skeletonDeploy="yes"
               noimports="no"
               verbose="no"
               testcase="yes">
        <mapping namespace="urn:import" package="test.wsdl.import"/>
        <mapping namespace="urn:importBinding1" package="test.wsdl.import.binding1"/>
        <mapping namespace="urn:importBinding2" package="test.wsdl.import.binding2"/>
